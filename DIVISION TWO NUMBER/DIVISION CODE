section .data
	num1 db 6
	num2 db 2
	result db 0
	newline db 10

section .text
	global _start

_start:

	mov al, [num1]   
	mov bl, [num2]   
	div bl
    
	add al, '0'

	mov [result], al

	mov eax,4
	mov ebx,1
	mov ecx, result
	mov edx,1
	int 80h
    
	mov eax,4
	mov ebx,1
	mov ecx, newline
	mov edx,1
	int 80h
    
mov eax,1
mov ebx,ebx
int 80h
